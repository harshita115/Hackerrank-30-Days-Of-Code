#include <bits/stdc++.h>

using namespace std;

int main()
{
    int num;
    cin >> num;
    if(num%2==0)
    {
        if(2<=num && num<=5)
           cout<<"Not weird ";
        else if(6<=num && num<=20)
           cout<<"Weird ";
        else
           cout<<"Not weird";
     }
    else
        cout<<"Weird";
    return 0;
}
